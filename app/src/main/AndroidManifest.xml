<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teacherassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShowStudentsData"></activity>
        <activity android:name=".ShowQuiz3Marks" />
        <activity android:name=".ShowQuiz2Marks" />
        <activity android:name=".ShowQuiz1Marks" />
        <activity android:name=".Quiz3Marks" />
        <activity android:name=".Quiz2Marks" />
        <activity android:name=".Quiz1Marks" />
        <activity android:name=".LoadData" />
        <activity android:name=".StoreData" />
        <activity android:name=".AddStudents" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>